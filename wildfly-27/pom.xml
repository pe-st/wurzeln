    </parent>

    <artifactId>wildfly-27</artifactId>
    <packaging>pom</packaging>

    <name>${project.groupId}.${project.artifactId}</name>

    <properties>
        <!-- latest EE10 spec -->
        <jakarta.jakartaee-api.version>10.0.0</jakarta.jakartaee-api.version>

        <!-- latest Wildfly 27 dependencies -->
        <!-- see https://github.com/wildfly/boms/releases -->
        <wildfly.bom.version>27.0.1.Final</wildfly.bom.version>

        <!-- Java EE modules that were removed from Java SE after Java 8 -->
        <jakarta.activation.version>2.1.1.jbossorg-1</jakarta.activation.version>
        <jakarta.xml.bind-api.version>4.0.0</jakarta.xml.bind-api.version>
        <jakarta.xml.soap-api.version>3.0.0</jakarta.xml.soap-api.version>
        <jakarta.xml.ws-api.version>4.0.0</jakarta.xml.ws-api.version>
        <jakarta.annotation.version>2.1.1</jakarta.annotation.version>
        <jakarta.transaction.version>2.0.1</jakarta.transaction.version>

        <!-- Jakarta EE modules where wildfly uses a "org.jboss.spec" dependency -->
        <!-- "org.jboss.spec.jakarta.el:jboss-el-api_5.0_spec" instead of "jakarta.el:jakarta.el-api", -->
        <!-- which uses org.glassfish:jakarta.el:5.0.0-M1 internally -->
        <version.org.jboss.spec.jakarta.el>4.0.0.Final</version.org.jboss.spec.jakarta.el>
        <jakarta.el.version>5.0.0-M1</jakarta.el.version>

        <!-- MicroProfile Standard Versions provided by Wildfly 27 -->
        <!-- see also https://mvnrepository.com/artifact/org.wildfly.bom/wildfly-microprofile/27.x.x.Final -->
        <mp.config.version>3.0.1</mp.config.version>
        <mp.fault-tolerance.version>4.0.2</mp.fault-tolerance.version>
        <mp.health.version>4.0</mp.health.version>
        <mp.jwt.version>2.0</mp.jwt.version>
        <mp.metrics.version>4.0</mp.metrics.version>
        <mp.openapi.version>3.0</mp.openapi.version>
        <mp.opentracing.version>3.0</mp.opentracing.version>
        <!-- mp.reactive-messaging classes included in smallrye.reactive-messaging, no dependency on upstream API -->
        <!-- but it is defined anyway in the wildfly-microprofile BOM -->
        <mp.reactive-messaging.version>3.0</mp.reactive-messaging.version>
        <mp.reactive-streams-operators.version>3.0</mp.reactive-streams-operators.version>
        <mp.rest-client.version>3.0</mp.rest-client.version>
        <smallrye.common.version>2.0.0</smallrye.common.version>
        <smallrye.config.version>3.0.0</smallrye.config.version>
        <smallrye.fault-tolerance.version>6.0.0</smallrye.fault-tolerance.version>
        <smallrye.health.version>4.0.0</smallrye.health.version>
        <smallrye.jwt.version>4.0.0</smallrye.jwt.version>
        <smallrye.metrics.version>4.0.0</smallrye.metrics.version>
        <smallrye.openapi.version>3.0.1</smallrye.openapi.version>
        <smallrye.opentracing.version>3.0.0</smallrye.opentracing.version>
        <smallrye.reactive-converter-api.version>2.6.0</smallrye.reactive-converter-api.version>
        <smallrye.reactive-messaging.version>4.0.0</smallrye.reactive-messaging.version>
        <smallrye.mutiny.reactive-streams-operators.version>1.7.0</smallrye.mutiny.reactive-streams-operators.version>
        <smallrye.mutiny.vertx.version>2.26.0</smallrye.mutiny.vertx.version>
