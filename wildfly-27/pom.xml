        <!-- tracer implementations -->
        <io.opentelemetry.version>1.12.0</io.opentelemetry.version>
        <io.opentracing.version>0.33.0</io.opentracing.version>
        <io.opentracing.contrib-jaxrs.version>1.0.0</io.opentracing.contrib-jaxrs.version>
        <io.jaegertracing.version>1.6.0</io.jaegertracing.version>

        <!-- Artifact versions provided by Wildfly 27.x.x.Final -->
        <!-- (see wildfly-27.x.x.Final/modules/system/layers/base and Wildfly BOM) -->
        <resteasy.version>6.2.1.Final</resteasy.version>
        <apache.httpclient.version>4.5.13</apache.httpclient.version>
        <!-- note that wildfly provides jakarta.json* in the path modules/system/layers/base/javax/json/* -->
        <jakarta.json.version>2.1.1</jakarta.json.version>
        <jakarta.json.bind.version>3.0.0</jakarta.json.bind.version>
        <parsson.version>1.1.1</parsson.version>
        <yasson.version>3.0.1</yasson.version>

        <!-- note that for new projects you should consider using JSON-B (yasson) instead of Jackson -->
        <jackson-2-version>2.13.4</jackson-2-version>
        <jackson-bom.version>2.13.4.20221013</jackson-bom.version>

        <!-- note that infinispan-directory-provider uses an older version -->
        <infinispan.version>14.0.2.Final</infinispan.version>
        <hibernate.version>6.1.5.Final</hibernate.version>

        <!-- Wildfly 27.x.x.Final uses Hibernate Validator 8.0.xx.Final -->
        <jakarta.validation.version>3.0.2</jakarta.validation.version>
        <hibernate.validator.version>8.0.0.Final</hibernate.validator.version>

        <!-- Wildfly 27.x.x.Final uses Weld 5.1.x (corresponding to CDI 4.0) -->
        <jakarta.inject.version>2.0.1</jakarta.inject.version>
        <!-- TODO use org.jboss.weld:weld-core-bom -->
        <weld.version>5.1.0.Final</weld.version>

        <!-- Logging API supported by Wildfly -->
        <slf4j.version>1.7.36</slf4j.version>

        <!-- Latest Wildfly plugin -->
        <wildfly.maven.plugin.version>${wildfly.plugin.version}</wildfly.maven.plugin.version>
    </properties>

    <dependencyManagement>
        <dependencies>
